{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_server-islands\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/_image\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/register\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/signin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/signout\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/login\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/dashboard\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/prueba\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/register\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/reporte\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/signin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/sorteo\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}